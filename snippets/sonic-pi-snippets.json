{
  "live_loop": {
    "scope": "ruby",
    "prefix": ["live_loop", "ll"],
    "body": "live_loop ${1::foo} do\n${2:what}\n${3:sleep 1}\nend"
  },

  "fx generic": {
    "scope": "ruby",
    "prefix": ["fx"],
    "body": "with_fx :${1:effect} do\n\t$2\nend"
  },

  "fx autotuner": {
    "scope": "ruby",
    "prefix": ["fx a"],
    "body": "with_fx :autotuner do\n\t$1\nend"
  },

